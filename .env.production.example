# Production Environment Variables for AWS Lightsail
# Copy this file to .env.production and update values

# -------------------------
# DOCKER & COMPOSE
# -------------------------
COMPOSE_PROJECT_NAME=inbola
POSTGRES_PASSWORD=your_secure_postgres_password_here
REDIS_PASSWORD=your_secure_redis_password_here

# -------------------------
# DOMAIN & SSL
# -------------------------
DOMAIN=inbola.uz
EMAIL=admin@inbola.uz

# -------------------------
# BACKUP SETTINGS
# -------------------------
BACKUP_RETENTION_DAYS=30
BACKUP_SCHEDULE="0 2 * * *"

# -------------------------
# MONITORING
# -------------------------
ENABLE_MONITORING=true
LOG_LEVEL=info
